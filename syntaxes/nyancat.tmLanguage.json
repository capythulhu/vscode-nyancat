{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "nyancat",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#values"
    },
    {
      "include": "#labels"
    },
    {
      "include": "#nums"
    },
    {
      "include": "#sys_bits"
    },
    {
      "include": "#bits"
    },
    {
      "include": "#qubits"
    },
    {
      "include": "#words"
    },
    {
      "include": "#params"
    },
    {
      "include": "#comments_block"
    },
    {
      "include": "#comments_line"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.nyancat",
          "match": "^[ ]*(end|put|mov|cmp|je|jne|jg|jge|jl|jle|jmp|add|sub|mul|div|m|h|x|y|z)\\b"
        }
      ]
    },
    "values": {
      "name": "entity.name.nyancat",
      "match": "[\\[\\]]"
    },
    "labels": {
      "name": "markup.bold.nyancat",
      "match": "[A-Z]([A-Za-z0-9_]*)\\w*"
    },
    "nums": {
      "name": "variable.name.nyancat",
      "match": "[0-9]{1,}( |$)"
    },
    "sys_bits": {
      "name": "entity.name.function.nyancat",
      "match": "[0-9]{1,}%"
    },
    "bits": {
      "name": "constant.character.nyanct",
      "match": "[0-9]{1,}!"
    },
    "qubits": {
      "name": "constant.numeric.nyancat",
      "match": "[0-9]{1,}\\?"
    },
    "words": {
      "name": "variable.name.nyancat",
      "match": "[a-z_]*([A-Za-z0-9_]*)\\w+"
    },
    "params": {
      "name": "variable.name.nyancat",
      "begin": "<",
      "beginCaptures": {
        "0": {
          "name": "variable.language"
        }
      },
      "end": ">",
      "endCaptures": {
        "0": {
          "name": "variable.language"
        }
      }
    },
    "comments_block": {
      "name": "comment.block.nyancat",
      "begin": "/\\*",
      "end": "\\*/"
    },
    "comments_line": {
      "name": "comment.line.double-slash.nyancat",
      "match": "(//.*)"
    }
  },
  "scopeName": "source.nyancat"
}