{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "nyancat",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#labels_define"
		},
		{
			"include": "#labels_use"
		},
		{
			"include": "#words"
		},
		{
			"include": "#nums"
		},
		{
			"include": "#sys_bits"
		},
		{
			"include": "#bits"
		},
		{
			"include": "#qubits"
		},
		{
			"include": "#params"
		},
		{
			"include": "#comments_block"
		},
		{
			"include": "#comments_line"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.nyancat",
				"match": "\\b(end|put|mov|cmp|je|jne|jg|jge|jl|jle|jmp|m|h|x|y|z)\\b"
			}]
		},
		"labels_define" : {
			"name": "markup.bold.nyancat",
			"match": "^[ ]*[A-Z]{1}([A-Za-z0-9_]*)\\w*"
		},
		"labels_use" : {
			"name": "markup.bold.nyancat",
			"match": "[ ]{1,}[A-Z]{1}([A-Za-z0-9_]*)\\w*"
		},
		"words" : {
			"name": "variable.name.nyancat",
			"match": "[a-zA-Z_]\\w*"
		},
		"nums" : {
			"name":"variable.name.nyancat",
			"match": "[0-9]{1,}( |$)"
		},
		"sys_bits" : {
			"name": "entity.name.function.nyancat",
			"match": "[0-9]{1,}%( |$)"
		},
		"bits" : {
			"name": "constant.character.nyanct",
			"match": "[0-9]{1,}!( |$)"
		},
		"qubits" : {
			"name": "constant.numeric.nyancat",
			"match": "[0-9]{1,}\\?( |$)"
		},
		"params": {
			"name": "variable.name.nyancat",
			"begin": "<",
			"beginCaptures": {
				"0": {
					"name": "variable.language"
				}
			},
			"end": ">",
			"endCaptures": {
				"0": {
					"name": "variable.language"
				}
			}
		},
		"comments_block": {
			"name": "comment.block.nyancat",
			"begin": "/\\*",
			"end": "\\*/"
		},
		"comments_line": {
			"name": "comment.line.double-slash.nyancat",
			"begin": "//",
			"end": "$\n?"
		}
	},
	"scopeName": "source.nyancat"
}